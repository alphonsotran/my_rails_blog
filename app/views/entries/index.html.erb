<h1>My Entries</h1>
<%= link_to 'New Entry', new_entry_path %>
<% @entries.each do |entry| %>
  <ul>
    <li><%= entry.title %> | <%= link_to 'Go', entry_path(entry) %> <%= link_to 'Edit', edit_entry_path(entry) %> <%= link_to 'Delete', entry_path(entry), method: :delete, data: {confirm: 'Are you sure you want to delete this entry?'} %></li>
  </ul>
<% end %>